{
  "name": "learn-react",
  "displayName": "React Docs",
  "description": "Learn React JS inside VS Code while building your react projects with quick access to https://www.react.dev documentation. Browse the docs quickly inside VS Code webviews",
  "keywords": [
    "React docs",
    "React docs in VS Code",
    "React"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "license": "MIT",
  "categories": [
    "Education",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "learn-react.view-resource",
        "title": "Learn React: View Resource at position"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format:write": "prettier --write 'src/**/*.{ts,tsx}'",
    "scrape": "node src/scripts/scrapper.js && pnpm format:write",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@babel/parser": "^7.21.4",
    "@babel/traverse": "^7.21.4",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "jsdom": "^22.0.0",
    "mocha": "^10.2.0",
    "node-fetch": "^2.6.7",
    "typescript": "^4.9.5",
    "vsce": "^2.15.0"
  }
}